import javax.swing.*;

public class lab3 {

	public static void main(String[] args) {
		String productNum = JOptionPane.showInputDialog("Input product name:");
		String smalldishstr = JOptionPane.showInputDialog
				("How many small dish :");
		int smalldish = Integer.parseInt(smalldishstr);
		
		String bigdishstr = JOptionPane.showInputDialog
				("How many big dish:");
		
		
		int bigdish = Integer.parseInt(bigdishstr);
		
		
		int smalldishprice=smalldish*30;
		int bigdishprice=bigdish*60;
		JOptionPane.showMessageDialog(null,"You order "+smalldish+" smalldish ("+smalldishprice+")"
				+ "\nYou order "+bigdish+" big dish " + "(" + bigdishprice + ")");

	}

}
